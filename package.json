{
  "name": "flowr-desktop",
  "version": "1.2.8",
  "author": {
    "email": "info@taktik.com",
    "name": "Taktik"
  },
  "description": "A PC client for Flowr",
  "keywords": [
    "web-browser",
    "material",
    "electron",
    "react",
    "mobx",
    "styled-components"
  ],
  "main": "build/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taktik/flowr-desktop.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV='dev' webpack --config webpack.main.js && webpack serve --config webpack.renderers.js",
    "build": "webpack --config webpack.main.js --env production && webpack --config webpack.renderers.js --env production",
    "setup": "npm install && npm run install-app-deps && npm run build",
    "setup-widevine": "npm install && npm run install-app-deps && npm run install-widevine && npm run copy-header && npm run build",
    "setup-widevine-win": "npm install && npm run install-app-deps && npm run install-widevine && npm run copy-header-win && npm run build",
    "start": "cross-env ENV='dev' electron . --inspect=127.0.0.1:3001",
    "compile-win32": "electron-builder --config electron-builder.common.js -w",
    "compile-darwin": "electron-builder --config electron-builder.common.js -m",
    "compile-linux": "electron-builder --config electron-builder.common.js -l",
    "compile-widevine-win32": "electron-builder --config electron-builder.widevine.js -w",
    "compile-widevine-darwin": "electron-builder --config electron-builder.widevine.js -m",
    "compile-widevine-linux": "electron-builder --config electron-builder.widevine.js -l",
    "lint": "tslint \"src/**/*.ts*\" --exclude \"src/wexond/**/*.ts*\" --project tsconfig.json",
    "lint-fix": "prettier --write \"src/**/*.ts*\" && tslint \"src/**/*.ts*\" --fix",
    "install-app-deps": "electron-builder install-app-deps",
    "install-widevine": "npm install castlabs/electron-releases#v11.1.1-wvvmp --no-save",
    "copy-header": "cp -r $HOME/.electron-gyp/11.1.1 $HOME/.electron-gyp/11.1.1-wvvmp",
    "copy-header-win": "cp -r %HOME%/.electron-gyp/11.1.1 %HOME%/.electron-gyp/11.1.1-wvvmp"
  },
  "pre-commit": [
    "lint-fix"
  ],
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/chrome": "0.0.83",
    "@types/deep-extend": "0.4.31",
    "@types/ffmpeg-static": "3.0.0",
    "@types/ffprobe-static": "2.0.0",
    "@types/fs-extra": "7.0.0",
    "@types/gsap": "1.20.2",
    "@types/leveldown": "4.0.0",
    "@types/levelup": "3.1.0",
    "@types/lodash": "4.14.132",
    "@types/nedb": "1.8.7",
    "@types/node": "12.0.0",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
    "@types/styled-components": "4.1.14",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "concurrently": "4.1.0",
    "copy-webpack-plugin": "6.2.0",
    "cross-env": "5.2.0",
    "css-loader": "4.3.0",
    "electron": "11.1.0",
    "electron-builder": "22.9.1",
    "file-loader": "6.1.0",
    "fuse-box": "3.7.1",
    "html-webpack-plugin": "4.5.0",
    "husky": "3.0.5",
    "prettier": "1.17.0",
    "raw-loader": "4.0.1",
    "style-loader": "1.3.0",
    "terser": "3.17.0",
    "terser-webpack-plugin": "4.2.3",
    "ts-loader": "8.0.4",
    "tslint": "5.16.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.0.5",
    "typescript-plugin-styled-components": "1.4.2",
    "url-loader": "4.1.0",
    "webpack": "5.3.1",
    "webpack-cli": "4.1.0",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "@cliqz/adblocker": "0.9.1",
    "@fortawesome/fontawesome-svg-core": "1.2.21",
    "@fortawesome/free-solid-svg-icons": "5.10.1",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@taktik/buffers": "1.0.4",
    "@taktik/flowr-common-js": "1.0.5",
    "@taktik/mux.js": "5.10.0",
    "@taktik/ts-decryptor": "1.0.10",
    "@taktik/udp-streamer": "1.0.9",
    "axios": "0.18.0",
    "crypto-random-string": "3.1.0",
    "deep-extend": "0.6.0",
    "electron-log": "3.0.5",
    "electron-updater": "4.0.6",
    "ffmpeg-static": "3.0.0",
    "ffprobe-static": "3.0.0",
    "file-type": "11.0.0",
    "fluent-ffmpeg": "git+https://github.com/lmancel/node-fluent-ffmpeg.git",
    "fs-extra": "8.0.1",
    "gsap": "2.1.2",
    "icojs": "0.12.3",
    "leveldown": "5.6.0",
    "levelup": "4.0.1",
    "lodash": "4.17.11",
    "mobx": "5.9.4",
    "mobx-react": "5.4.3",
    "moment": "2.24.0",
    "mouse-hooks": "0.3.1",
    "nedb": "1.8.0",
    "network-everywhere": "1.0.3",
    "node-vibrant": "3.2.0-alpha",
    "node-window-manager": "2.2.4",
    "pretty-bytes": "5.2.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "run-parallel": "1.1.9",
    "simple-keyboard": "2.30.29",
    "styled-components": "4.2.0",
    "tldts": "4.0.6",
    "typescript-state-machine": "0.9.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
